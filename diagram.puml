@startuml
!pragma layout elk
skinparam classAttributeIconSize 0
class DataSource {
}
interface Repository {
  save(entity : T) : void
}
class User {
  id : UUID
  name : String
  getName() : String
}
class UserRepository {
  dataSource : DataSource
  save(user : User) : void
}
Repository <|.. UserRepository
class UmlGuiApp {
  sourcePathField : TextField
  outputPumlField : TextField
  pumlArea : TextArea
  legendList : ListView<String>
  preview : ImageView
  status : Label
  useElkLayout : CheckBox
  generateBtn : Button
  exportPngBtn : Button
  tempWorkDir : Path
  lastSymbolClicked : String
  lastMatchIndex : int
  UML_SYMBOLS : Map<String, String>
  start(stage : Stage) : void
  chooseFolder(stage : Stage) : void
  generateUml() : void
  exportPng(stage : Stage) : void
  unzipToTemp(zip : Path, tempDir : Path) : Path
  highlightNextSymbolInPuml(symbol : String) : void
  extractRelationship(text : String, symbol : String, index : int) : String
  describeRelationship(left : String, arrow : String, right : String) : String
  setStatus(msg : String) : void
  main(args : String[]) : void
}
Application <|-- UmlGuiApp
class Main {
  main(args : String[]) : void
}
class UmlModel {
  types : Map<String, UmlType>
  associations : List<String[]>
}
class UmlType {
  name : String
  kind : Kind
  fields : List<String>
  methods : List<String>
  extendsTypes : List<String>
  implementsTypes : List<String>
}
enum Kind {
}
class PlantUmlRenderer {
  toPlantUml(model : UmlModel) : String
}
UserRepository --> DataSource
UmlType --> Kind
@enduml
